{
  "name": "svg-to-tsx-react-native-cli",
  "version": "0.0.7",
  "description": "Based on svg-to-tsx-react-cli-cli. A command line utility that takes a svg image file and outputs a fully formatted stateless functional React Native component with `height` and `width` for props. With flags to toggle formatting and remove style attributes.",
  "main": "index.js",
  "dependencies": {
    "chalk": "^1.1.3",
    "htmltojsx": "^0.3.0",
    "jsdom-no-contextify": "^3.1.0",
    "svg-to-jsx": "^0.0.21",
    "yargs": "^6.5.0"
  },
  "scripts": {
    "test": "node index.js --dir ./src/test-svgs/svgs --output ./src/test-svgs/components",
    "pre": "node index.js --dir ./src/test-svgs/svgs --output ./src/test-svgs/components --pre"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/movibe/svg-to-tsx-react-native-cli.git"
  },
  "keywords": [
    "react",
    "react-native",
    "svg",
    "component"
  ],
  "author": "Willian Ribeiro Angelo <agfoccus@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/movibe/svg-to-tsx-react-native-cli/issues"
  },
  "homepage": "https://github.com/movibe/svg-to-tsx-react-native-cli#readme",
  "bin": {
    "svg-to-tsx-react-native": "./index.js"
  }
}
